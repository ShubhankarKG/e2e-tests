generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
  previewFeatures = ["napi"]
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:posgres@localhost:5432/postgres=?schema=public"
}

model User {
  id    String  @default(cuid()) @id
  email String  @unique
  name  String?
}
