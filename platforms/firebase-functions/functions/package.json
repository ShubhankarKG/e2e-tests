{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "postinstall": "npx cross-env CI=1 && npx cross-env PRISMA_TELEMETRY_INFORMATION=\"e2e-tests platforms/firebase-functions postinstall\" && yarn prisma generate"
  },
  "engines": {
    "node": "12"
  },
  "dependencies": {
    "@prisma/client": "2.20.0-dev.20",
    "firebase-admin": "9.5.0",
    "firebase-functions": "3.13.2"
  },
  "devDependencies": {
    "firebase-functions-test": "0.2.3",
    "prisma": "2.20.0-dev.20",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
  }
}
